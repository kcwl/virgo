enum color
{
    red;
    black;
};

struct person
{
    int age;
    string name;
};

protocol hello {
    request {
        router:
            tcp-key 1001;

        header:
            int id;
            
        body:
            int id;
        };
    response {
        router:
        tcp-key 1001;

    header:
        int id;
            
    body:
        int id;
    };
};